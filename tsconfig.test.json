{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "lib": ["DOM", "ES2022"],
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "strictPropertyInitialization": false
  },
  "include": ["next-env.d.ts", "**/?(*.)+(spec|test|it).ts"],
}
